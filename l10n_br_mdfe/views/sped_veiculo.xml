<odoo>

    <record model="ir.ui.view" id="sped_veiculo_form_view">
        <field name="name">l10n_br_delivery.carrier.vehicle.form (in l10n_br_mdfe)</field>
        <field name="model">l10n_br_delivery.carrier.vehicle</field>
        <field name="inherit_id" ref="l10n_br_delivery.l10n_br_delivery_carrier_vehicle_form"/>
        <field name="arch" type="xml">
            <group name="shipping_company" position="after">
                <group name="vehicle">
                    <field name="wheeled_type"/>
                    <field name="body_type"/>
                    <field name="ciot"/>
                    <field name="tare_kg"/>
                    <field name="capacity_kg"/>
                    <field name="capacity_m3"/>
                </group>
            </group>
        </field>
    </record>

